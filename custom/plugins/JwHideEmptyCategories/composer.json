{
	"name": "jw/hide-empty-categories",
	"description": "Hide empty categories",
	"version": "1.0.0",
	"type": "shopware-platform-plugin",
	"require": {
		"shopware/core": "^6.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.4"
	},
	"autoload": {
		"psr-4": {
			"Jw\\HideEmptyCategories\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Jw\\HideEmptyCategories\\Test\\": "tests/"
		}
	},
	"extra": {
		"shopware-plugin-class": "Jw\\HideEmptyCategories\\JwHideEmptyCategories",
		"label": {
			"en-GB": "Plugin for hiding empty categories"
		}
	}
}
